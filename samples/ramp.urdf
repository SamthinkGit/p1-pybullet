<?xml version="1.0"?>
<robot name="box">

    <!-- [Base] Size 4 x 2 x 2 -->
    <!-- Parent: base -->
    <!-- Center: 0 0 0.6 -->
    <link name="base">

        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <box size="4 2 2"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
                
        </visual>

        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <box size="4 2 2"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0"/>
            <mass value="300.0"/>
            <inertia ixx="300.0" ixy="0.0" ixz="0.0" iyy="300.0" iyz="0.0" izz="300.0"/>
        </inertial>
    </link>

    <!-- Joint [base2rightramp] -->
    <!-- Parent: base -->
    <!-- New-Center: 2 1 1 (Y-Rotated) -->
    <joint name="base2rightramp" type="fixed">
        <parent link="base"/>
        <child link="right_ramp"/>
        <origin xyz="2 0 1" rpy="0.0 0.588 0.0"/>
    </joint>

    <!-- [Right_Ramp] Size 3.60 x 2 x 0.02 -->
    <!-- Parent: base -->
    <!-- Parent-Center: Local(0 0 0) Global(2 1 1) (Rotated)-->
    <!-- Center: Local(1.78 0 -0.05) -->
    <link name="right_ramp">

        <visual>
            <origin xyz="1.78 0 -0.01"/>
            <geometry>
                <box size="3.60 2.0 0.02"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
                
        </visual>

        <collision>
            <origin xyz="1.78 0 -0.01"/>
            <geometry>
                <box size="3.60 2.0 0.02"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="1.78 0 -0.01"/>
            <mass value="100.0"/>
            <inertia ixx="100.0" ixy="0.0" ixz="0.0" iyy="100.0" iyz="0.0" izz="100.0"/>
        </inertial>
    </link>

    <!-- Joint [base2leftramp] -->
    <!-- Parent: base -->
    <!-- New-Center: 2 1 1 (Y-Rotated) -->
    <joint name="base2leftramp" type="fixed">
        <parent link="base"/>
        <child link="left_ramp"/>
        <origin xyz="-2 0 1" rpy="0.0 -0.5880 0.0"/>
    </joint>

    <!-- [Right_Ramp] Size 3.60 x 2 x 0.02 -->
    <!-- Parent: base -->
    <!-- Parent-Center: Local(0 0 0) Global(-2 1 1) (Rotated)-->
    <!-- Center: Local(-1.78 0 -0.05) -->
    <link name="left_ramp">

        <visual>
            <origin xyz="-1.78 0 -0.01"/>
            <geometry>
                <box size="3.60 2.0 0.02"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
                
        </visual>

        <collision>
            <origin xyz="-1.78 0 -0.01"/>
            <geometry>
                <box size="3.60 2.0 0.02"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="-1.78 0 -0.01"/>
            <mass value="100.0"/>
            <inertia ixx="100.0" ixy="0.0" ixz="0.0" iyy="100.0" iyz="0.0" izz="100.0"/>
        </inertial>
    </link>
</robot>